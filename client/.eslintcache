:\n      return {\n        ...state,\n        current: null\n      };\n    case FILTER_CONTACTS:\n      return {\n        ...state,\n        filtered: state.contacts.filter(contact => {\n          const regex = new RegExp(`${action.payload}`, 'gi');\n          return contact.name.match(regex) || contact.email.match(regex)\n        })\n      };\n    case CLEAR_FILTER:\n      return {\n        ...state,\n        fltered: null\n      }\n    default:\n      return state;\n  }\n};\n","/home/prince/full-stack/contacts-keeper/client/src/components/contacts/Contacts.js",[],"/home/prince/full-stack/contacts-keeper/client/src/components/contacts/ContactForm.js",[],"/home/prince/full-stack/contacts-keeper/client/src/context/contact/contactContext.js",[],"/home/prince/full-stack/contacts-keeper/client/src/context/types.js",[],"/home/prince/full-stack/contacts-keeper/client/src/components/contacts/ContactItem.js",[],"/home/prince/full-stack/contacts-keeper/client/src/components/contacts/ContactFilter.js",["100"],"import React, { useContext, useRef, useEffect } from 'react';\nimport ContactContext from '../../context/contact/contactContext';\nimport ContactState from '../../context/contact/contactState';\n\nconst ContactFilter = () => {\n    const contactContext = useContext(ContactContext);\n    const text = useRef('');\n\n    const { filterContacts, clearFilter, filtered } = contactContext;\n\n    useEffect(() => {\n        if (filtered === null) {\n            text.current.value = '';\n        }\n    });\n\n    const onChange = e => {\n        if (text.current.value !== '') {\n            filterContacts(e.target.value);\n        } else {\n            clearFilter();\n        }\n    };\n\n    return (\n        <form>\n            <input\n                ref={text}\n                type='text'\n                placeholder='Filter Contacts...'\n                onChange={onChange}\n            />\n        </form>\n    );\n};\n\nexport default ContactFilter;\n","/home/prince/full-st